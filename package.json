{
	"name": "@ctx-core/instagram_cache",
	"version": "10.0.10",
	"description": "instagram scraper, account cache, & gallery component",
	"homepage": "https://github.com/ctx-core/instagram_cache#readme",
	"type": "module",
	"types": "./src/index.ts",
	"exports": {
		".": {
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rimraf dist && npm run clean_tsbuildinfo",
		"clean_tsbuildinfo": "rm -f tsconfig.tsbuildinfo && rm -f dist/**/*.d.ts",
		"compile": "npm run compile_source && npm run compile_declaration",
		"compile_source": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"compile_declaration": "npm run clean_tsbuildinfo && tsc --declaration --emitDeclarationOnly --declarationDir dist",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build",
		"start": "./bin/instagram_cache.js"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.44.0",
		"@ctx-core/array": "workspace:^23.0.40",
		"@ctx-core/dom": "workspace:^11.0.47",
		"@ctx-core/fetch": "workspace:^11.0.100",
		"@ctx-core/object": "workspace:^22.0.7",
		"@ctx-core/store": "workspace:^27.0.54"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.55",
		"@swc/core": "^1.2.120",
		"rimraf": "^3.0.2",
		"typescript": "next"
	},
	"svelte": "./dist/index.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/instagram_cache.git"
	},
	"bugs": {
		"url": "https://github.com/ctx-core/instagram_cache/issues"
	}
}
