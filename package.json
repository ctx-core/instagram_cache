{
	"name": "@ctx-core/instagram_cache",
	"version": "7.0.69",
	"description": "instagram scraper, account cache, & gallery component",
	"homepage": "https://github.com/ctx-core/instagram_cache#readme",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"scripts": {
		"build": "npm run clean && npm run generate_ctx_I_file && npm run copy-components && npm run compile",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -f src/**/*.js && rm -f src/**/*.d.ts && rm -f src/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"copy-components": "cd src && find . | grep \\.svelte$ | awk '{print \"mkdir -p dirname \"$1\" && cp -f \"$1\" ../dist/\"$1}'",
		"exec": "$@",
		"generate_ctx_I_file": "./node_modules/.bin/generate_ctx_I_file",
		"prepublishOnly": "npm run build",
		"start": "./bin/instagram_cache.js"
	},
	"dependencies": {
		"@aws-sdk/client-dynamodb": "^3.21.0",
		"@aws-sdk/client-s3": "^3.21.0",
		"@ctx-core/array": "workspace:^22.3.0",
		"@ctx-core/ctx-core-package-tools": "workspace:^8.0.24",
		"@ctx-core/dom": "workspace:^10.0.27",
		"@ctx-core/fetch": "workspace:^10.0.42",
		"@ctx-core/fontawesome": "workspace:^14.0.27",
		"@ctx-core/function": "workspace:^19.2.0",
		"@ctx-core/instagram": "workspace:^5.0.32",
		"@ctx-core/object": "workspace:^20.5.0",
		"@ctx-core/sleep": "workspace:^7.0.10",
		"@ctx-core/store": "workspace:^26.0.27",
		"chromedriver": "^91.0.1",
		"command-line-args": "^5.1.3",
		"esm": "^3.2.25",
		"jsdom": "^16.6.0",
		"selenium-webdriver": "^4.0.0-beta.4",
		"svelte": "3.38.3",
		"url-parse": "^1.5.1"
	},
	"devDependencies": {
		"@ctx-core/dev-tools": "workspace:^6.0.16",
		"@types/jsdom": "^16.2.13",
		"@types/selenium-webdriver": "^4.0.15",
		"typescript": "^4.3.5"
	}
}