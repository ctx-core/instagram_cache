{
	"name": "@ctx-core/instagram_cache",
	"version": "10.0.7",
	"description": "instagram scraper, account cache, & gallery component",
	"homepage": "https://github.com/ctx-core/instagram_cache#readme",
	"type": "module",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./package.json": "./package.json"
	},
	"bin": {
		"cache-instagram": "./bin/cache-instagram.mjs"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rimraf dist",
		"compile": "npm run compile_source && npm run compile_declaration",
		"compile_source": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"compile_declaration": "tsc src/index.ts --declaration --emitDeclarationOnly --declarationDir dist",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build",
		"start": "./bin/instagram_cache.js"
	},
	"dependencies": {
		"@aws-sdk/client-dynamodb": "^3.44.0",
		"@aws-sdk/client-s3": "^3.44.0",
		"@ctx-core/array": "workspace:^23.0.37",
		"@ctx-core/ctx-core-package-tools": "workspace:^8.0.91",
		"@ctx-core/dom": "workspace:^11.0.44",
		"@ctx-core/fetch": "workspace:^11.0.97",
		"@ctx-core/fontawesome": "workspace:^16.0.5",
		"@ctx-core/function": "workspace:^20.2.16",
		"@ctx-core/instagram": "workspace:^6.0.97",
		"@ctx-core/instagram_cache": "workspace:^10.0.7",
		"@ctx-core/object": "workspace:^22.0.4",
		"@ctx-core/sleep": "workspace:^8.0.28",
		"@ctx-core/store": "workspace:^27.0.51",
		"chromedriver": "^96.0.0",
		"command-line-args": "^5.2.0",
		"jsdom": "^19.0.0",
		"selenium-webdriver": "^4.1.0",
		"svelte": "^3.44.3",
		"url-parse": "^1.5.3"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.55",
		"@swc/core": "^1.2.120",
		"@types/jsdom": "^16.2.13",
		"@types/selenium-webdriver": "^4.0.16",
		"rimraf": "^3.0.2",
		"typescript": "next"
	},
	"svelte": "./dist/index.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/instagram_cache.git"
	},
	"bugs": {
		"url": "https://github.com/ctx-core/instagram_cache/issues"
	}
}
