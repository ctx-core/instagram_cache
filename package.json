{
	"name": "@ctx-core/instagram_cache",
	"version": "8.0.134",
	"description": "instagram scraper, account cache, & gallery component",
	"homepage": "https://github.com/ctx-core/instagram_cache#readme",
	"type": "module",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"bin": {
		"cache-instagram": "./bin/cache-instagram.mjs"
	},
	"scripts": {
		"build": "npm run clean && npm run copy-mjs && npm run generate_ctx_I_file && npm run copy-components && npm run compile",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -f src/**/*.js && rm -f src/**/*.d.ts && rm -f src/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"copy-mjs": "cd dist && echo \"module.exports = (async () => import('./index.js'))()\" > index.cjs",
		"copy-components": "cd src && find . | grep \\.svelte$ | awk '{print \"mkdir -p dirname \"$1\" && cp -f \"$1\" ../dist/\"$1}'",
		"exec": "$@",
		"generate_ctx_I_file": "./node_modules/.bin/generate_ctx_I_file",
		"prepublishOnly": "npm run build",
		"start": "./bin/instagram_cache.js"
	},
	"dependencies": {
		"@aws-sdk/client-dynamodb": "^3.39.0",
		"@aws-sdk/client-s3": "^3.39.0",
		"@ctx-core/array": "workspace:^23.0.22",
		"@ctx-core/ctx-core-package-tools": "workspace:^8.0.77",
		"@ctx-core/dom": "workspace:^11.0.28",
		"@ctx-core/fetch": "workspace:^11.0.78",
		"@ctx-core/fontawesome": "workspace:^15.0.59",
		"@ctx-core/function": "workspace:^20.2.2",
		"@ctx-core/instagram": "workspace:^6.0.76",
		"@ctx-core/instagram_cache": "workspace:^8.0.134",
		"@ctx-core/object": "workspace:^21.0.21",
		"@ctx-core/sleep": "workspace:^8.0.14",
		"@ctx-core/store": "workspace:^27.0.35",
		"chromedriver": "^95.0.0",
		"command-line-args": "^5.2.0",
		"esm": "^3.2.25",
		"jsdom": "^18.0.0",
		"selenium-webdriver": "^4.0.0",
		"svelte": "3.44.1",
		"url-parse": "^1.5.3"
	},
	"devDependencies": {
		"@ctx-core/dev-tools": "workspace:^7.1.4",
		"@types/jsdom": "^16.2.13",
		"@types/selenium-webdriver": "^4.0.16",
		"typescript": "^4.4.4"
	}
}