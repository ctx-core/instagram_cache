{
	"name": "@ctx-core/instagram_cache",
	"version": "8.0.156",
	"description": "instagram scraper, account cache, & gallery component",
	"homepage": "https://github.com/ctx-core/instagram_cache#readme",
	"type": "module",
	"module": "./dist/index.js",
	"types": "./src/index.ts",
	"exports": {
		".": {
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"bin": {
		"cache-instagram": "./bin/cache-instagram.mjs"
	},
	"scripts": {
		"build": "npm run generate_ctx_I_file && npm run compile",
		"clean": "rm -rf dist",
		"compile": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"exec": "$@",
		"generate_ctx_I_file": "swc $(./node_modules/.bin/generate_ctx_I_file | tail -n1) --out-dir dist --source-maps --config-file .swcrc",
		"prepublishOnly": "npm run clean && npm run build",
		"start": "./bin/instagram_cache.js"
	},
	"dependencies": {
		"@aws-sdk/client-dynamodb": "^3.44.0",
		"@aws-sdk/client-s3": "^3.44.0",
		"@ctx-core/array": "workspace:^23.0.32",
		"@ctx-core/ctx-core-package-tools": "workspace:^8.0.85",
		"@ctx-core/dom": "workspace:^11.0.39",
		"@ctx-core/fetch": "workspace:^11.0.92",
		"@ctx-core/fontawesome": "workspace:^15.1.13",
		"@ctx-core/function": "workspace:^20.2.12",
		"@ctx-core/instagram": "workspace:^6.0.92",
		"@ctx-core/instagram_cache": "workspace:^8.0.156",
		"@ctx-core/object": "workspace:^21.0.30",
		"@ctx-core/sleep": "workspace:^8.0.23",
		"@ctx-core/store": "workspace:^27.0.45",
		"chromedriver": "^96.0.0",
		"command-line-args": "^5.2.0",
		"jsdom": "^19.0.0",
		"selenium-webdriver": "^4.1.0",
		"svelte": "3.44.2",
		"url-parse": "^1.5.3"
	},
	"devDependencies": {
		"@ctx-core/dev-tools": "workspace:^7.2.9",
		"@swc/cli": "^0.1.52",
		"@swc/core": "^1.2.118",
		"@types/jsdom": "^16.2.13",
		"@types/selenium-webdriver": "^4.0.16",
		"typescript": "next"
	}
}
