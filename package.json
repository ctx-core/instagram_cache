{
	"name": "@ctx-core/instagram_cache",
	"version": "11.0.7",
	"description": "instagram scraper, account cache, & gallery component",
	"homepage": "https://github.com/ctx-core/instagram_cache#readme",
	"type": "module",
	"types": "./src/index.d.ts",
	"svelte": "./src/index.js",
	"exports": {
		".": {
			"import": "./src/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build && npm run test",
		"test": "pnpm test-unit && check-dts",
		"test-unit": "tsm node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"test-unit-coverage": "c8 pnpm test-unit"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.45.0",
		"@ctx-core/array": "workspace:^23.1.2",
		"@ctx-core/dom": "workspace:^11.1.3",
		"@ctx-core/fetch": "workspace:^11.0.106",
		"@ctx-core/nanostores": "workspace:^0.3.2",
		"@ctx-core/object": "workspace:^22.0.10",
		"@ctx-core/svelte": "workspace:^10.1.3"
	},
	"devDependencies": {
		"c8": "^7.11.0",
		"check-dts": "^0.6.6",
		"tsm": "^2.2.1",
		"typescript": "next",
		"uvu": "^0.5.2"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/instagram_cache.git"
	},
	"bugs": {
		"url": "https://github.com/ctx-core/instagram_cache/issues"
	}
