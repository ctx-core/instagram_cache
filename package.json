{
	"name": "@ctx-core/instagram_cache",
	"version": "11.0.5",
	"description": "instagram scraper, account cache, & gallery component",
	"homepage": "https://github.com/ctx-core/instagram_cache#readme",
	"type": "module",
	"types": "./lib/index.d.ts",
	"exports": {
		".": {
			"import": "./lib/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rimraf lib && npm run clean:tsbuildinfo",
		"clean:tsbuildinfo": "rm -f tsconfig.tsbuildinfo && rm -f lib/**/*.d.ts",
		"compile": "npm run compile:source && npm run compile:declaration",
		"compile:source": "swc src --out-dir lib --copy-files --source-maps --config-file .swcrc",
		"compile:declaration": "npm run clean:tsbuildinfo && tsc --declaration --emitDeclarationOnly --declarationDir lib",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build",
		"start": "./bin/instagram_cache.js"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.45.0",
		"@ctx-core/array": "workspace:^23.1.0",
		"@ctx-core/dom": "workspace:^11.1.0",
		"@ctx-core/fetch": "workspace:^11.0.102",
		"@ctx-core/nanostores": "workspace:^0.3.0",
		"@ctx-core/object": "workspace:^22.0.8",
		"@ctx-core/svelte": "workspace:^10.1.1"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.55",
		"@swc/core": "^1.2.123",
		"rimraf": "^3.0.2",
		"typescript": "next"
	},
	"svelte": "./lib/index.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/instagram_cache.git"
	},
	"bugs": {
		"url": "https://github.com/ctx-core/instagram_cache/issues"
	}
}