{
	"name": "@ctx-core/instagram_cache",
	"version": "15.1.14",
	"description": "instagram scraper, account cache, & gallery component",
	"homepage": "https://github.com/ctx-core/instagram_cache#readme",
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"./_types",
		"./medium_pathname",
		"./medium_pathname_a",
		"./medium_pathname_idx",
		"./s3__medium_pathname_a",
		"./source__medium_pathname_a"
	],
	"types": "./index.d.ts",
	"exports": {
		".": "./index.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm test-unit && check-dts",
		"test-unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"test-unit-coverage": "c8 pnpm test-unit"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.451.0",
		"@ctx-core/array": "^27.0.10",
		"@ctx-core/function": "^21.22.5",
		"@ctx-core/dom": "^15.1.10",
		"@ctx-core/env": "^17.4.10",
		"@ctx-core/fetch-undici": "^3.1.14",
		"@ctx-core/nanostores": "^7.1.11",
		"@ctx-core/object": "^30.4.2",
		"@ctx-core/svelte": "^14.2.10"
	},
	"devDependencies": {
		"c8": "^8.0.1",
		"check-dts": "^0.7.2",
		"tsx": "^4.1.3",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/instagram_cache.git"
	},
	"bugs": {
		"url": "https://github.com/ctx-core/instagram_cache/issues"
	},
	"sideEffects": false
}