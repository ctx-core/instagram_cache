{
	"name": "@ctx-core/instagram_cache",
	"version": "13.0.12",
	"description": "instagram scraper, account cache, & gallery component",
	"homepage": "https://github.com/ctx-core/instagram_cache#readme",
	"type": "module",
	"types": "./src/index.d.ts",
	"svelte": "./src/index.js",
	"exports": {
		".": {
			"import": "./src/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build && npm run test",
		"test": "pnpm test-unit && check-dts",
		"test-unit": "tsm node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"test-unit-coverage": "c8 pnpm test-unit"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.51.0",
		"@ctx-core/array": "*",
		"@ctx-core/dom": "^11.1.7",
		"@ctx-core/fetch-undici": "^1.2.1",
		"@ctx-core/nanostores": "^0.7.1",
		"@ctx-core/object": "*",
		"@ctx-core/svelte": "^10.1.8"
	},
	"devDependencies": {
		"c8": "^7.11.0",
		"check-dts": "^0.6.6",
		"tsm": "^2.2.1",
		"typescript": "next",
		"uvu": "^0.5.3"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/instagram_cache.git"
	},
	"bugs": {
		"url": "https://github.com/ctx-core/instagram_cache/issues"
	},
	"sideEffects": false
