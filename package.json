{
	"name": "@ctx-core/instagram_cache",
	"version": "10.0.0",
	"description": "instagram scraper, account cache, & gallery component",
	"homepage": "https://github.com/ctx-core/instagram_cache#readme",
	"type": "module",
	"exports": {
		".": {
			"types": "./src/index.ts",
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"bin": {
		"cache-instagram": "./bin/cache-instagram.mjs"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rm -rf dist",
		"compile": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build",
		"start": "./bin/instagram_cache.js"
	},
	"dependencies": {
		"@aws-sdk/client-dynamodb": "^3.44.0",
		"@aws-sdk/client-s3": "^3.44.0",
		"@ctx-core/array": "workspace:^23.0.33",
		"@ctx-core/ctx-core-package-tools": "workspace:^8.0.85",
		"@ctx-core/dom": "workspace:^11.0.40",
		"@ctx-core/fetch": "workspace:^11.0.93",
		"@ctx-core/fontawesome": "workspace:^16.0.0",
		"@ctx-core/function": "workspace:^20.2.12",
		"@ctx-core/instagram": "workspace:^6.0.93",
		"@ctx-core/instagram_cache": "workspace:^10.0.0",
		"@ctx-core/object": "workspace:^22.0.0",
		"@ctx-core/sleep": "workspace:^8.0.24",
		"@ctx-core/store": "workspace:^27.0.46",
		"chromedriver": "^96.0.0",
		"command-line-args": "^5.2.0",
		"jsdom": "^19.0.0",
		"selenium-webdriver": "^4.1.0",
		"svelte": "3.44.2",
		"url-parse": "^1.5.3"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.55",
		"@swc/core": "^1.2.120",
		"@types/jsdom": "^16.2.13",
		"@types/selenium-webdriver": "^4.0.16",
		"typescript": "next"
	}
}
