{
	"name": "@ctx-core/instagram_cache",
	"version": "8.0.138",
	"description": "instagram scraper, account cache, & gallery component",
	"homepage": "https://github.com/ctx-core/instagram_cache#readme",
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./src/index.ts",
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"bin": {
		"cache-instagram": "./bin/cache-instagram.mjs"
	},
	"scripts": {
		"build": "npm run generate_ctx_I_file && npm run compile",
		"clean": "rm -rf dist",
		"compile": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"exec": "$@",
		"generate_ctx_I_file": "swc $(./node_modules/.bin/generate_ctx_I_file | tail -n1) --out-dir dist --source-maps --config-file .swcrc",
		"prepublishOnly": "npm run build",
		"start": "./bin/instagram_cache.js"
	},
	"dependencies": {
		"@aws-sdk/client-dynamodb": "^3.40.0",
		"@aws-sdk/client-s3": "^3.40.1",
		"@ctx-core/array": "workspace:^23.0.23",
		"@ctx-core/ctx-core-package-tools": "workspace:^8.0.77",
		"@ctx-core/dom": "workspace:^11.0.30",
		"@ctx-core/fetch": "workspace:^11.0.80",
		"@ctx-core/fontawesome": "workspace:^15.1.0",
		"@ctx-core/function": "workspace:^20.2.3",
		"@ctx-core/instagram": "workspace:^6.0.78",
		"@ctx-core/instagram_cache": "workspace:^8.0.138",
		"@ctx-core/object": "workspace:^21.0.22",
		"@ctx-core/sleep": "workspace:^8.0.15",
		"@ctx-core/store": "workspace:^27.0.36",
		"chromedriver": "^95.0.0",
		"command-line-args": "^5.2.0",
		"jsdom": "^18.0.1",
		"selenium-webdriver": "^4.0.0",
		"svelte": "3.44.1",
		"url-parse": "^1.5.3"
	},
	"devDependencies": {
		"@ctx-core/dev-tools": "workspace:^7.2.0",
		"@swc/cli": "^0.1.51",
		"@swc/core": "^1.2.107",
		"@types/jsdom": "^16.2.13",
		"@types/selenium-webdriver": "^4.0.16",
		"typescript": "^4.4.4"
	}
}
